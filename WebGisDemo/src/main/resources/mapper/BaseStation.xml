<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.luxx.web.dao.StationMapper">
    <resultMap id="baseStationResultMap" type="com.luxx.web.model.Station">
        <id property="id" column="id"/>
        <result property="countryCode" column="country_code"/>
        <result property="provider" column="provider"/>
        <result property="lac" column="lac"/>
        <result property="cell" column="cell"/>
        <result property="latitude" column="latitude"/>
        <result property="longitude" column="longitude"/>
        <result property="range" column="range"/>
        <result property="address" column="address"/>
    </resultMap>

    <!-- select语句 -->
    <select id="getAllBaseStation" resultMap="baseStationResultMap">
       select id,country_code,provider,lac,cell,latitude,longitude,range,address
       from station
    </select>

</mapper>

